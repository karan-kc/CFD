FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 29.5480411312;

boundaryField
{
    inlet
    {
        type            turbulentMixingLengthFrequencyInlet;
        mixingLength	0.014; //0.07*Diameter of pipe (Here D=H=0.2)
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }
    fixedWalls-bottom
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    fixedWalls-top
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
