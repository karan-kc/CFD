FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.001;

vertices
(
    (0   0 -0.5)   //0
    (20  0 -0.5)   //1

    (0   2 -0.5)   //2
    (20  2 -0.5)   //3
//
    (0   0  0.5)   //4
    (20  0  0.5)   //5

    (0   2  0.5)   //6
    (20  2  0.5)   //7
);

blocks
(
    hex (0 1 3 2 4 5 7 6) (600 60 1) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    inlet
    {
        type patch;
        faces
        (
            (0 4 6 2)
        );
    }
    outlet
    {
        type patch;
        faces
        (
            (1 3 7 5)
        );
    } 
    fixedWalls-bottom
    {
        type wall;
        faces
        (
            (2 6 7 3)
        );
    }
    fixedWalls-top
    {
        type wall;
        faces
        (
            (0 1 5 4)
        );
    }

    frontAndBack //openFoam omits using this face as its type is empty
    {
        type empty;
        faces
        (
        (0 2 3 1)
	    (4 5 7 6)
        );
    }
);

mergePatchPairs
(
);

