FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2000";
    object      wallHeatFlux;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    fixedWalls-bottom
    {
        type            calculated;
        value           nonuniform List<scalar> 2(-116.073 -116);
    }
    fixedWalls-top
    {
        type            calculated;
        value           nonuniform List<scalar> 2(116.072 116);
    }
    frontAndBack
    {
        type            empty;
    }
}


