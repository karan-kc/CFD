FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "2000";
    object      wallShearStress;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    fixedWalls-bottom
    {
        type            calculated;
        value           nonuniform List<vector> 2((-0.00666713 -2.60712e-07 0) (-0.00664022 6.31323e-07 0));
    }
    fixedWalls-top
    {
        type            calculated;
        value           nonuniform List<vector> 2((-0.00834079 -9.84652e-07 0) (-0.00831241 1.00512e-06 0));
    }
    frontAndBack
    {
        type            empty;
    }
}


